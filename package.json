{
  "name": "back",
  "version": "16.15.0",
  "description": "endoh.io - server",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=512 npm run migrate && node build/server.js",
    "dev": "ts-node-dev src/server.ts",
    "watch": "node --inspect=9229 -r ts-node/register  ./src/server.ts ",
    "build": "tsc",
    "makeMigrations": "npm run typeorm migration:generate  -n ./src/migrations/Migration",
    "makeEmptyMigration": "npm run typeorm migration:create -n ./src/migrations/Migration",
    "migrate": "npm run typeorm migration:run",
    "revertMigration": "npm run typeorm migration:revert",
    "test-server": "set NODE_ENV=test&&  nodemon",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles",
    "test:watch": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles",
    "swagger": "tsoa spec",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/dataSource.ts"
  },
  "dependencies": {
    "@babel/core": "^7.22.8",
    "@sendgrid/mail": "^7.7.0",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "avsc": "^5.7.7",
    "aws-sdk": "^2.1412.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-automatic-routes": "^1.1.0",
    "express-openid-connect": "^2.16.0",
    "express-partial-response": "^1.0.4",
    "got": "^13.0.0",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "ioredis": "^5.3.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.1",
    "kafkajs": "^2.2.4",
    "luxon": "^3.3.0",
    "metascraper": "^5.34.8",
    "metascraper-description": "^5.34.7",
    "metascraper-image": "^5.34.7",
    "metascraper-title": "^5.34.7",
    "multer": "^1.4.2",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.3.1",
    "node-notifier": "^10.0.1",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.1",
    "puppeteer": "^8.0.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "routing-controllers": "^0.10.4",
    "socket.io": "^4.7.1",
    "sqlite3": "^5.1.6",
    "stripe": "^12.11.0",
    "swagger-ui-express": "^4.6.3",
    "type-graphql": "^2.0.0-beta.2",
    "typeorm": "^0.3.17",
    "typeorm-pagination": "^2.0.3",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^20.4.0",
    "@types/node-fetch": "^2.6.4",
    "@types/node-notifier": "^8.0.2",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/response-time": "^2.3.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm run watch",
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  }
}
