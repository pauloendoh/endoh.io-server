    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/uuid": "^8.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "avsc": "^5.7.4",
    "aws-sdk": "^2.867.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "express-automatic-routes": "^1.1.0",
    "express-openid-connect": "^2.6.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.0.0",
    "luxon": "^1.27.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.6.0",
    "puppeteer": "^8.0.0",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^4.4.1",
