    "typeorm-pagination": "^2.0.3",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "typeorm migration:run && node build/server.js",
    "start:producer": "ts-node-dev --transpile-only --respawn --inspect=4322 --project tsconfig.json src/kafka-test/producer.ts",
    "start:consumer": "ts-node-dev --transpile-only --respawn src/kafka-test/consumer.ts",
    "watch": "node --inspect=5757 -r ts-node/register  ./src/server.ts ",
    "dev": "nodemon",
    "build": "tsc",
    "makeMigrations": "ts-node node_modules/typeorm/cli.js migration:generate -n Migration",
    "makeEmptyMigration": "ts-node node_modules/typeorm/cli.js migration:create -n Migration",
    "migrate": "ts-node node_modules/typeorm/cli.js migration:run",
    "revertMigration": "ts-node node_modules/typeorm/cli.js migration:revert",
    "test-server": "set NODE_ENV=test&&  nodemon",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles",
    "test:watch": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles",
    "runKafka": "docker compose up"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run watch",
    "ext": "ts"
  }
}
