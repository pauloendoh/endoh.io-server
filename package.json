{
  "name": "back",
  "version": "16.15.0",
  "description": "endoh.io - server",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=512 npm run migrate && node -r newrelic build/server.js",
    "dev": "ts-node-dev src/server.ts",
    "watch": "node --inspect=9229 -r ts-node/register  ./src/server.ts ",
    "build": "tsc",
    "makeMigrations": "npm run typeorm migration:generate  -n ./src/migrations/Migration",
    "makeEmptyMigration": "npm run typeorm migration:create -n ./src/migrations/Migration",
    "migrate": "npm run typeorm migration:run",
    "revertMigration": "npm run typeorm migration:revert",
    "test-server": "set NODE_ENV=test&&  nodemon",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles",
    "test:watch": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/dataSource.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.7",
    "@aws-sdk/client-s3": "^3.693.0",
    "@babel/core": "^7.26.0",
    "@sendgrid/mail": "^7.7.0",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "apollo-server": "^3.13.0",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "avsc": "^5.7.7",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-automatic-routes": "^1.1.0",
    "express-openid-connect": "^2.17.1",
    "express-partial-response": "^1.0.4",
    "got": "^13.0.0",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "ioredis": "^5.4.1",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "luxon": "^3.5.0",
    "metascraper": "^5.45.25",
    "metascraper-description": "^5.45.25",
    "metascraper-image": "^5.45.25",
    "metascraper-title": "^5.45.25",
    "multer": "^1.4.4",
    "multer-s3": "^3.0.1",
    "newrelic": "^12.18.2",
    "node-fetch": "^3.3.2",
    "node-notifier": "^10.0.1",
    "nodemailer": "^6.9.16",
    "openai": "^4.89.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.1",
    "puppeteer": "^8.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "response-time": "^2.3.3",
    "routing-controllers": "^0.10.4",
    "socket.io": "^4.8.1",
    "stripe": "^12.18.0",
    "swagger-ui-express": "^5.0.1",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3",
    "url-metadata": "^4.1.1",
    "uuid": "^9.0.1",
    "yaml": "^2.8.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cookie-session": "^2.0.49",
    "@types/crypto-js": "^4.2.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.17.6",
    "@types/node-fetch": "^2.6.12",
    "@types/node-notifier": "^8.0.5",
    "@types/nodemailer": "^6.4.16",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/response-time": "^2.3.8",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm run watch",
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  }
}
