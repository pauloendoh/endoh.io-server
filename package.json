{
  "name": "back",
  "version": "14.16.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/cookie-session": "^2.0.42",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.5.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.6"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/luxon": "^1.26.5",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "aws-sdk": "^2.867.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-automatic-routes": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.27.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.6.0",
    "puppeteer": "^8.0.0",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.2.1",
    "typeorm": "^0.2.37",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "typeorm migration:run && node build/server.js",
    "watch": "node --inspect=5757 -r ts-node/register  ./src/server.ts ",
    "dev": "nodemon",
    "build": "tsc",
    "makeMigrations": "ts-node node_modules/typeorm/cli.js migration:generate -n Migration",
    "migrate": "ts-node node_modules/typeorm/cli.js migration:run",
    "revertMigration": "ts-node node_modules/typeorm/cli.js migration:revert",
    "test-server": "set NODE_ENV=test&&  nodemon",
    "test": "set NODE_ENV=test&& jest",
    "test:watch": "set NODE_ENV=test&& jest --watchAll"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run watch",
    "ext": "ts"
  }
}
