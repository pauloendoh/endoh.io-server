{
  "name": "back",
  "version": "10.15.3",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-session": "^2.0.41",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^8.0.29",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "bcrypt": "^5.0.0",
    "sqlite3": "^5.0.0",
    "class-transformer": "^0.3.1",
    "express": "^4.17.1",
    "dotenv": "^8.2.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "jsonwebtoken": "^8.5.1",
    "ts-node": "^3.3.0",
    "typeorm": "0.2.28",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "start": "ts-node node_modules/typeorm/cli.js migration:run && NODE_ENV=production ts-node src/server.ts",
    "watch": "node --inspect=5757 -r ts-node/register ./src/server.ts",
    "dev": "nodemon",
    "build": "tsc", 

    "makeMigrations": "ts-node node_modules/typeorm/cli.js migration:generate -n Migration", 
    "migrate": "ts-node node_modules/typeorm/cli.js migration:run",
    "revertMigration": "ts-node node_modules/typeorm/cli.js migration:revert"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run watch",
    "ext": "ts"
  }
}
