{
  "name": "back",
  "version": "16.15.0",
  "description": "endoh.io - server",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=512 npm run migrate && node build/server.js",
    "dev": "ts-node-dev src/server.ts",
    "watch": "node --inspect=9229 -r ts-node/register  ./src/server.ts ",
    "build": "tsc",
    "makeMigrations": "npm run typeorm migration:generate  -n ./src/migrations/Migration",
    "makeEmptyMigration": "npm run typeorm migration:create -n ./src/migrations/Migration",
    "migrate": "npm run typeorm migration:run",
    "revertMigration": "npm run typeorm migration:revert",
    "test-server": "set NODE_ENV=test&&  nodemon",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles",
    "test:watch": "set NODE_ENV=test&& jest --watchAll --detectOpenHandles",
    "swagger": "tsoa spec",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/dataSource.ts"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@sendgrid/mail": "^7.6.2",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "avsc": "^5.7.4",
    "aws-sdk": "^2.1247.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "express-automatic-routes": "^1.1.0",
    "express-openid-connect": "^2.6.0",
    "express-partial-response": "^1.0.4",
    "got": "^12.1.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.17.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.0.0",
    "luxon": "^1.27.0",
    "metascraper": "^5.29.8",
    "metascraper-description": "^5.29.7",
    "metascraper-image": "^5.29.7",
    "metascraper-title": "^5.29.7",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.6.0",
    "puppeteer": "^8.0.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.10",
    "response-time": "^2.3.2",
    "routing-controllers": "^0.9.0",
    "socket.io": "^4.4.1",
    "sqlite3": "^5.0.2",
    "stripe": "^10.11.0",
    "swagger-ui-express": "^4.4.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.10",
    "typeorm-pagination": "^2.0.3",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/luxon": "^2.0.5",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/node": "^16.11.1",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/response-time": "^2.3.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "concurrently": "^7.2.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm run watch",
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  }
}
